{
  "name": "twilio-run",
  "version": "2.0.0-beta.8",
  "bin": {
    "twilio-functions": "./bin/twilio-run.js",
    "twilio-run": "./bin/twilio-run.js"
  },
  "description": "CLI tool to run Twilio Functions locally for development",
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc",
    "build:noemit": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "jest": "jest",
    "test": "run-s build:noemit jest",
    "contrib:add": "all-contributors add",
    "contrib:generate": "all-contributors generate"
  },
  "keywords": [
    "twilio",
    "twilio-functions",
    "serverless"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/twilio-labs/twilio-run.git"
  },
  "bugs": {
    "url": "https://github.com/twilio-labs/twilio-run/issues",
    "email": "dkundel@twilio.com"
  },
  "author": "Dominik Kundel <dkundel@twilio.com>",
  "license": "MIT",
  "dependencies": {
    "@twilio-labs/serverless-api": "^1.0.0-alpha.11",
    "@twilio-labs/serverless-runtime-types": "^1.0.1",
    "@types/express": "^4.17.0",
    "body-parser": "^1.18.3",
    "boxen": "^1.3.0",
    "chalk": "^2.4.2",
    "columnify": "^1.5.4",
    "common-tags": "^1.8.0",
    "debug": "^3.1.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.3",
    "got": "^9.6.0",
    "inquirer": "^6.0.0",
    "is-ci": "^2.0.0",
    "listr": "^0.14.3",
    "lodash.camelcase": "^4.3.0",
    "lodash.startcase": "^4.4.0",
    "log-symbols": "^2.2.0",
    "ngrok": "^3.0.1",
    "ora": "^3.3.1",
    "pkg-install": "^1.0.0",
    "prompts": "^2.0.4",
    "title": "^3.4.1",
    "twilio": "^3.18.0",
    "type-fest": "^0.6.0",
    "window-size": "^1.1.1",
    "wrap-ansi": "^5.1.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "all-contributors-cli": "^5.3.0",
    "cheerio": "^1.0.0-rc.2",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "jest-express": "^1.10.1",
    "lint-staged": "9.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "supertest": "^3.1.0",
    "ts-jest": "^24.0.2",
    "typescript": "^3.5.2"
  },
  "files": [
    "bin/",
    "dist/",
    "LICENSE",
    "README.md"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "npm test",
      "git add"
    ]
  }
}
