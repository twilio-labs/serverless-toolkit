{
  "name": "@twilio-labs/serverless-api",
  "version": "1.0.0-rc.5",
  "description": "API-wrapper for the Twilio Serverless API",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "jest": "jest",
    "test": "run-s build:noemit jest",
    "build": "tsc",
    "build:noemit": "tsc --noEmit",
    "postbuild": "npm shrinkwrap",
    "docs": "typedoc --options typedoc.json",
    "clean": "rimraf ./dist",
    "prepack": "run-s clean build",
    "lint-staged": "lint-staged",
    "cm": "git-cz",
    "release": "HUSKY_SKIP_HOOKS=1 standard-version",
    "reset-dependencies": "bash ./hooks/update-dependencies"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/twilio-labs/serverless-api.git"
  },
  "keywords": [
    "twilio",
    "serverless"
  ],
  "author": "Dominik Kundel <hi@dominik.dev> (https://dkundel.com)",
  "license": "MIT",
  "files": [
    "dist/",
    "!dist/**/__tests__/*",
    "README.md",
    "scripts/",
    "npm-shrinkwrap.json",
    "LICENSE"
  ],
  "devDependencies": {
    "@commitlint/cli": "^8.0.0",
    "@commitlint/config-conventional": "^8.0.0",
    "@types/debug": "^4.1.3",
    "@types/jest": "^24.0.16",
    "@types/nock": "^10.0.3",
    "all-contributors-cli": "^6.7.0",
    "commitizen": "^3.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "husky": "^3.0.0",
    "jest": "^24.8.0",
    "lint-staged": "^8.2.1",
    "nock": "^10.0.6",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.18.2",
    "rimraf": "^2.6.3",
    "standard-version": "^6.0.1",
    "ts-jest": "^24.0.2",
    "typedoc": "^0.14.2",
    "typedoc-plugin-external-module-name": "^2.0.0",
    "typescript": "^3.4.2"
  },
  "dependencies": {
    "@types/form-data": "^2.5.0",
    "@types/got": "^9.4.1",
    "@types/mime-types": "^2.1.0",
    "@types/node": "^11.13.2",
    "@types/recursive-readdir": "^2.2.0",
    "debug": "^4.1.1",
    "fast-redact": "^1.5.0",
    "file-type": "^10.10.0",
    "form-data": "^2.5.0",
    "got": "^9.6.0",
    "mime-types": "^2.1.22",
    "recursive-readdir": "^2.2.2",
    "type-fest": "^0.3.0",
    "upath": "^1.1.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "run-s lint-staged test",
      "prepare-commit-msg": "exec < /dev/tty && git-cz --hook",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "post-checkout": "reset-dependencies",
      "post-merge": "reset-dependencies"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
